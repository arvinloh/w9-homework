{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 800,
  "height": 400,
  "title": "Map of Australia showing live births in 2021 by state/territory",
  "projection": { "type": "equalEarth" },
  "data": {
    "url": "js/ne_1.json",
    "format": { "type": "topojson", "feature": "ne_10m_admin_1_states_provinces" }
  },
  "transform": [
    {
      "lookup": "properties.STATE_NAME",
      "from": {
        "data": {
          "url": "data.csv"
        },
        "key": "State/territory of birth",
        "fields": ["Live births"]
      }
    }
  ],
  "mark": { "type": "geoshape", "stroke": "black", "strokeWidth": 1 },
  "encoding": {
    "color": {
      "field": "Live births",
      "type": "quantitative",
      "scale": { "type": "log" }
    },
    "tooltip": [
      {
        "field": "properties.STATE_NAME",
        "type": "nominal",
        "title": "State/Territory"
      },
      { "field": "Live births", "type": "quantitative" }
    ]
  }
}
